{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "prettier": true,
    "no-console": [true, "log"],
    "jsx-no-lambda": false,
    "only-arrow-functions": false,
    "ordered-imports": false,
    "interface-name": false
  }
}
